{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    "customManagers:dockerfileVersions",
    ":disableRateLimiting",
    ":reviewer(Flo0807)"
  ],
  "packageRules": [
    {
      "groupName": "Elixir Image",
      "matchDepNames": [
        "elixir",
        "erlang",
        "debian"
      ]
    }
  ],
  "labels": [
    "dependencies",
    "{{categories}}"
  ]
}